#! /bin/bash

# 00_how_to_docker - 25
# Q: Lister lâ€™ensemble des services du swarm local

set -o errexit

if [ "$#" -gt 0 ]; then
	echo Usage: "${0}"
	exit 1
fi

(set -x; docker service ls)

